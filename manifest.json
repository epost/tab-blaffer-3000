{
  "manifest_version": 3,

  "name": "Erik's Tab Blaffer 3000",
  "description": "Browser tab related stuff",
  "version": "1.0",

  "action": {
    "default_icon": "icon.png",
    "default_title": "Dump tabs",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://ajax.googleapis.com/"
  ]
}
