{
  "manifest_version": 2,

  "name": "Erik's Tab Blaffer 3000",
  "description": "Browser tab related stuff",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
